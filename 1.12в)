import java.util.Scanner;

public class Main {
    public static void main(String[] args) {;
        Scanner scanner = new Scanner(System.in);

        //Введите  значение х1 с клавиатуры. в значение b вводим число 25,  значение x2 с клавиатуры и в значение y вводим число с клавиатуры  по условию задачи
        System.out.print("Введите люболе число x 1:");
        int x = scanner.nextInt();
        int b = 25;
        System.out.print("Введите люболе число x 2:");
        int j = scanner.nextInt();
        System.out.print("Введите люболе число y:");
        int y= scanner.nextInt();

        System.out.print(x+" "+b);
        System.out.println();
        System.out.print(j+" "+y);

    }
}
